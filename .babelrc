{
  "presets": [
    [
      "@babel/env",
      {
        "useBuiltIns": "usage",
        "targets": {
          "browsers": [">1%", "not ie 11", "not op_mini all"]
        }
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/transform-runtime",
    "react-hot-loader/babel"
  ]
}
